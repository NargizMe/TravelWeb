<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Travel</title>
    <!-- CSS -->
    <link rel="stylesheet" href="style.css" />
    <!-- GOOGLE FONTS -->
    <link
      href="https://fonts.googleapis.com/css?family=Montserrat&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="container">
      <section class="top-container">
        <!-- NAVBAR -->
        <nav class="top-nav">
          <img src="./img/logo.jpg" alt="" />
          <div class="top-menu">
            <a href="#" class="top-menu-link">FAQS</a>
            <a href="#" class="top-menu-link">ABOUT US</a>
            <a href="#footer" class="top-menu-link contact">CONTACT</a>
            <a href="#" class="top-menu-link">USD</a>
          </div>
        </nav>
        <!-- BANNER -->
        <div class="banner">
          <div class="flyes">
            <table style="width:100%" class="fly-table">
              <tr>
                <th>From</th>
                <th>To</th> 
                <th>Departure</th>
                <th>Return</th>
              </tr>
            </table>
            <button class="close">CLOSE</button>
          </div>
          <div class="banner-menu">
            <a href="#" class="banner-menu-link home">HOME</a>
            <a href="#" class="banner-menu-link flights">FLIGHTS</a>
            <a href="#" class="banner-menu-link">HOTELS</a>
            <a href="#" class="banner-menu-link">HOLIDAYS</a>
            <a href="#blogs" class="banner-menu-link blog-path">BLOG</a>
          </div>
          <!-- BANNER CARD -->
          <div class="banner-card">
            <div class="banner-card-top-btns">
              <a href="#" class="domestic-btn">Domestic</a>
              <a href="#" class="international-btn">International</a>
            </div>
            <div class="banner-card-inputs">
              <div class="card-input-container">
                <label for="">FROM</label>
                <input
                  type="text"
                  name="from"
                  class="card-data"
                  id=""
                  placeholder="Paris, France"
                />
              </div>
              <div class="card-input-container">
                <label for="">TO</label>
                <input
                  type="text"
                  name="to"
                  class="card-data"
                  id=""
                  placeholder="New-York, United State"
                />
              </div>
              <div class="card-input-container">
                <label for="">DEPARTURE</label>
                <input
                  type="text" name="departure"
                  class="card-data"
                  id="card-data-select"
                  placeholder="01.08.2011"
                  onfocus="(this.type='date')"
                >
            </select>
              </div>
              <div class="card-input-container">
                <label for="">RETURN</label>
                <input
                  type="text" name="return"
                  class="card-data"
                  id="card-data-select"
                  placeholder="23.09.2013"
                  onfocus="(this.type='date')"
                >
        </select>
              </div>
              <div class="card-input-container">
                <label for="">ADULTS</label>
                <select name="adults" class="card-data" id="card-data-select">
                  <option value="">1 adult</option>
                  <option value="">2 adults</option>
                  <option value="">more than 2 adults</option>
                </select>
              </div>
              <div class="card-input-container">
                <label for="">CHILDREN</label>
                <select name="children" class="card-data" id="card-data-select">
                  <option value="">No children</option>
                  <option value="">1 child</option>
                  <option value="">more than 1 child</option>
                </select>
              </div>
            </div>
            <!-- <a href='#' class="banner-card-bottom-btn">SEARCH FLIGHTS</a> -->
            <button class="banner-card-bottom-btn">SEARCH FLIGHTS</button>
          </div>
          <!-- BANNER FEATURES -->
          <div class="banner-features">
            <div class="banner-feature">
            <i class="fas fa-plane-departure"></i>
            <small>Easy & Cheap</small>
            <p>SEARCH FLOGHTS</p>
            </div>
            <div class="banner-feature">
            <i class="fas fa-key"></i>
            <small>Over 15000+ Places</small>
            <p>GET HOTEL DEALS</p>
            </div>
            <div class="banner-feature">
            <i class="fas fa-gift"></i>
            <small>Explore Destinations</small>
            <p>HOLIDAY PACKAGES</p>
            </div>
            <div class="banner-feature">
            <i class="fas fa-headphones-alt"></i>
            <small>24x7 Dedicated Helpline</small>
            <p>1800 105 25 41</p>
            </div>
          </div>
        </div>
